import java.util.Scanner;

class VowelCheck
{
	public static void main(String...s)
	{
		Scanner sc = new Scanner(System.in);
	
		System.out.print("Enter Alphabet : ");
		char operation = sc.next().charAt(0);
		
		switch(operation)
		{
			case 'a':
			System.out.println(operation+" is a vowel");
			break;
			
			case 'e':
			System.out.println(operation+" is a vowel");
			break;
			
			case 'i':
			System.out.println(operation+" is a vowel");
			break;
			
			case '0':
			System.out.println(operation+" is a vowel");
			break;
			
			case 'u':
			System.out.println(operation+" is a vowel");
			break;
			
			default:
			System.out.println(operation+" is a vowel");
		}
	}
}
